{
  "name": "ADSWalletrn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:se2": "npx react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "init:dev:ru": "cp ./internals/.env.ru.development ./.env",
    "init:prod:ru": "cp ./internals/.env.ru.prod ./.env",
    "init:dev:usa": "cp ./internals/.env.usa.development ./.env",
    "init:prod:usa": "cp ./internals/.env.usa.prod ./.env",
    "start": "react-native start",
    "pods": "cd ios && pod install && cd ..",
    "pods-fabric": "cd ios && RCT_NEW_ARCH_ENABLED=1 pod install && cd ..",
    "process-api-types": "npx ts-node-esm  ./scripts/process-types-from-json.ts",
    "renaming-refactor": "npx ts-node-esm  ./scripts/renaming-refactor.ts",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "find-deadcode": "ts-prune | grep -v -E '(used in module)'",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.jsx,.ts,.tsx",
    "plop": "npx plop",
    "prepare": "husky install",
    "deps-cruise": "npx depcruise src --include-only \"^src\" --config --output-type dot | dot -T svg > dependency-graph.svg",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
