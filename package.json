    "@react-navigation/stack": "^6.3.10",
    "@reduxjs/toolkit": "^1.9.2",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.4",
    "axios": "^1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "patch-package": "^6.5.0",
